<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mobiledoormanapp.core" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="CloudFivePush">
        <param name="android-package" value="com.cloudfiveapp.cordova.plugins.push.CloudFivePush" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="File">
        <param name="android-package" value="org.apache.cordova.file.FileUtils" />
        <param name="onload" value="true" />
    </feature>
    <feature name="FileTransfer">
        <param name="android-package" value="org.apache.cordova.filetransfer.FileTransfer" />
    </feature>
    <feature name="Sms">
        <param name="android-package" value="org.apache.cordova.plugin.sms.Sms" />
    </feature>
    <name>Mobile Doorman</name>
    <description>
        Apartment building management app
    </description>
    <author email="brian@tenforwardconsulting.com" href="http://tenforwardconsulting.com">
        Ten Forward Consulting, Inc
    </author>
    <content src="index.html" />
    <access origin="*" />
    <gcmSenderId>69583903294</gcmSenderId>
</widget>
